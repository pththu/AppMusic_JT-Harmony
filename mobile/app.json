{
  "expo": {
    "name": "JT Harmony",
    "slug": "mobile_jt-harmony",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo_final_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#a7f29b"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_mini.png",
        "backgroundColor": "#a7f29b"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.junia03.mobile_jtharmony",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-document-picker",
      "expo-av",
      [
        "expo-image-picker",
        {
          "photosPermission": "Ứng dụng cần quyền truy cập thư viện ảnh để upload hình ảnh"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d5918968-3bf6-466a-b726-07b812636bd3"
      },
      "API_URL": "https://jt-harmony.vercel.app//api/v1/",
      "SOCKET_SERVER_URL": "https://jt-harmony-socket.vercel.app/",
      "GOOGLE_OAUTH_CLIENT_ID_APP": "145190840857-ild54e2gkkbjas6f2km11b2s156mj05c.apps.googleusercontent.com"
    }
  }
}